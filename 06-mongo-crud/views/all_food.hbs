{{#extends 'base'}}

{{#block 'content'}}
<table class="table">
    <thead>
        <tr>
            <th>TD</th>
            <th>Name</th>
            <th>Calories</th>
            <th>Tags</th>
        </tr>
    </thead>
    {{#each foodRecords}}
        <tr>
            <td>{{this._id}}</td>
            <td>{{this.name}}</td>
            <td>{{this.calories}}</td>
            <td>{{this.tags}}</td>
        </tr>
    {{/each}}
</table>
{{/block}}

{{/extends}}